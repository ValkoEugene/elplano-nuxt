{
  "name": "elplano-nuxt",
  "version": "1.0.0",
  "description": "My stupendous Nuxt.js project",
  "author": "Valko Eugene",
  "private": true,
  "scripts": {
    "dev": "yarn && cross-env BASE_URL=https://elplano-api.herokuapp.com PORT=3000 nuxt",
    "build": "yarn && cross-env BASE_URL=https://elplano-api.herokuapp.com nuxt build",
    "analyze": "yarn && nuxt build --analyze",
    "deploy-manual": "./deploy_manual.sh",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "80"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.2.1",
    "@vuex-orm/core": "^0.32.4",
    "axios": "0.18.0",
    "js-cookie": "^2.2.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "nuxt": "2.10.1",
    "nuxt-material-design-icons": "^1.0.4",
    "shrink-ray-current": "^4.1.2",
    "vue-content-loader": "^0.2.2",
    "vue-i18n": "^8.15.0",
    "vue-infinite-scroll": "^2.0.2",
    "vue-property-decorator": "^8.2.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.1",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@nuxtjs/vuetify": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "moment-locales-webpack-plugin": "^1.1.0",
    "prettier": "^1.16.4",
    "vue-class-component": "^7.1.0",
    "vue-jest": "^4.0.0-0"
  }
