{
  "name": "elplano-nuxt",
  "version": "1.0.0",
  "description": "My stupendous Nuxt.js project",
  "author": "Valko Eugene",
  "private": true,
  "scripts": {
    "dev": "yarn && cross-env BASE_URL=https://elplano-api.herokuapp.com PORT=3000 nuxt",
    "build": "yarn && cross-env BASE_URL=https://elplano-api.herokuapp.com nuxt build",
    "analyze": "yarn && nuxt build --analyze",
    "deploy-manual": "./deploy_manual.sh",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "80"
    }
  },
  "dependencies": {
    "@vuex-orm/core": "^0.32.2",
    "axios": "0.18.0",
    "js-cookie": "^2.2.1",
    "lodash.clonedeep": "^4.5.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "nuxt": "^2.0.0",
    "nuxt-material-design-icons": "^1.0.4",
    "shrink-ray-current": "^4.0.0",
    "vue-content-loader": "^0.2.2",
    "vue-i18n": "^8.14.0",
    "vue-infinite-scroll": "^2.0.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/vuetify": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.1.0",
    "moment-locales-webpack-plugin": "^1.1.0",
    "prettier": "^1.16.4",
    "vue-jest": "^4.0.0-0"
  }
}
